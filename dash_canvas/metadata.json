{
  "src/lib/components/DashCanvas.react.js": {
    "description": "Canvas component for drawing on a background image and selecting\nregions.",
    "displayName": "DashCanvas",
    "methods": [],
    "props": {
      "id": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "The ID used to identify this component in Dash callbacks"
      },
      "image_content": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "Image data string, formatted as png or jpg data string. Can be\ngenerated by utils.io_utils.array_to_data_string.",
        "defaultValue": {
          "value": "''",
          "computed": false
        }
      },
      "zoom": {
        "type": {
          "name": "number"
        },
        "required": false,
        "description": "Zoom factor",
        "defaultValue": {
          "value": "1",
          "computed": false
        }
      },
      "width": {
        "type": {
          "name": "number"
        },
        "required": false,
        "description": "Width of the canvas",
        "defaultValue": {
          "value": "500",
          "computed": false
        }
      },
      "height": {
        "type": {
          "name": "number"
        },
        "required": false,
        "description": "Height of the canvas",
        "defaultValue": {
          "value": "500",
          "computed": false
        }
      },
      "scale": {
        "type": {
          "name": "number"
        },
        "required": false,
        "description": "Scaling ratio between canvas width and image width",
        "defaultValue": {
          "value": "1",
          "computed": false
        }
      },
      "tool": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "Selection of drawing tool, among [\"pencil\", \"pan\", \"circle\",\n\"rectangle\", \"select\", \"line\"].",
        "defaultValue": {
          "value": "\"pencil\"",
          "computed": false
        }
      },
      "lineWidth": {
        "type": {
          "name": "number"
        },
        "required": false,
        "description": "Width of drawing line (in pencil mode)",
        "defaultValue": {
          "value": "10",
          "computed": false
        }
      },
      "lineColor": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "Color of drawing line (in pencil mode). Can be a text string,\nlike 'yellow', 'red', or a color triplet like 'rgb(255, 0, 0)'.\nAlpha is possible with 'rgba(255, 0, 0, 0.5)'.",
        "defaultValue": {
          "value": "'red'",
          "computed": false
        }
      },
      "goButtonTitle": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "Title of button",
        "defaultValue": {
          "value": "'Save'",
          "computed": false
        }
      },
      "filename": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "Name of image file to load (URL string)",
        "defaultValue": {
          "value": "''",
          "computed": false
        }
      },
      "trigger": {
        "type": {
          "name": "number"
        },
        "required": false,
        "description": "Counter of how many times the save button was pressed\n(to be used mostly as input)",
        "defaultValue": {
          "value": "0",
          "computed": false
        }
      },
      "json_data": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "Sketch content as JSON string, containing background image and\nannotations. Use utils.parse_json.parse_jsonstring to parse\nthis string.",
        "defaultValue": {
          "value": "''",
          "computed": false
        }
      },
      "hide_buttons": {
        "type": {
          "name": "arrayOf",
          "value": {
            "name": "string"
          }
        },
        "required": false,
        "description": "Names of buttons to hide. Names are \"zoom\", \"pan\", \"line\", \"pencil\",\n\"rectangle\", \"undo\", \"select\".",
        "defaultValue": {
          "value": "[]",
          "computed": false
        }
      },
      "setProps": {
        "type": {
          "name": "func"
        },
        "required": false,
        "description": "Dash-assigned callback that should be called whenever any of the\nproperties change"
      }
    }
  }
}
